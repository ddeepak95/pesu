# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Pipecat Configuration
# Set to 'true' to use cloud endpoint, 'false' to use local endpoint
NEXT_PUBLIC_PIPECAT_USE_CLOUD=false
# Local Pipecat endpoint (for development)
NEXT_PUBLIC_PIPECAT_LOCAL_ENDPOINT=http://localhost:7860/start
# Cloud Pipecat endpoint (for production or cloud testing)
NEXT_PUBLIC_PIPECAT_CLOUD_ENDPOINT=https://api.pipecat.daily.co/v1/public/pesu-pipecat/start
# API key for Pipecat Cloud (only required when NEXT_PUBLIC_PIPECAT_USE_CLOUD=true)
NEXT_PUBLIC_PIPECAT_API_KEY=your_pipecat_api_key

